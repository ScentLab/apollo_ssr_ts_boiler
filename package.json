{
  "name": "apollo_ssr_ts_boiler",
  "version": "0.1.0",
  "main": "server.js",
  "repository": "https://github.com/vung2/apollo_ssr_ts_boiler.git",
  "author": "rayleighko <gmj1197@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npx dotenv-cli -e .env.prod -- next start",
    "dev": "npx dotenv-cli -e .env.dev -- node ./server.js",
    "build": "npx dotenv-cli -e .env.prod -- npx rimraf ./src/.next && next build ./src",
    "build:debug": "npx dotenv-cli -e .env.dev -- npx rimraf ./src/.next && next build ./src",
    "deploy": "npx dotenv-cli -e .env.prod -- sh ./scripts/deploy-prod.sh input_aws_profile_name",
    "deploy:debug": "npx dotenv-cli -e .env.dev -- sh ./scripts/deploy-dev.sh input_aws_profile_name",
    "generate": "npx graphql-codegen --config codegen.yml",
    "generate:pre": "npx graphql-cli --dotenv .env.dev get-schema",
    "test:unit": "jest",
    "test:e2e": "cypress",
    "test:all": "yarn test:unit && yarn test:e2e"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.14",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.14",
    "next": "^9.0.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cypress": "^3.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  }
}
